apiVersion: v1
kind: Service
metadata:
  name: consul-dns-proxy
  namespace: kube-system
  labels:
    kubernetes.io/cluster-service: "true"
    furnace-app: consul-dns-proxy
spec:
  selector:
    furnace-app: consul-dns-proxy
  clusterIP: <%= p("kube.consul_dns_proxy_address") %>
  ports:
  - name: dns
    port: 9053
    protocol: UDP
  - name: dns-tcp
    port: 9053
    protocol: TCP
