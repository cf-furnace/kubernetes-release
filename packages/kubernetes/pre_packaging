#!/bin/bash
set -eux

k8s_version=1.3.0

mkdir -p ${BUILD_DIR}/tmp
tar zxvf kubernetes/kubernetes-${k8s_version}.tar.gz -C ${BUILD_DIR}/tmp kubernetes/server/kubernetes-server-linux-amd64.tar.gz
ls -lR ${BUILD_DIR}
mv ${BUILD_DIR}/tmp/kubernetes/server/kubernetes-server-linux-amd64.tar.gz ${BUILD_DIR}/kubernetes

rm -rf ${BUILD_DIR}/tmp kubernetes/kubernetes-${k8s_version}.tar.gz
